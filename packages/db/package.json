{
  "name": "@hazel/db",
  "private": true,
  "type": "module",
  "scripts": {
    "db:push": "infisical run -- drizzle-kit push:mysql",
    "db:pull": "infisical run -- drizzle-kit introspect:mysql",
    "db:generate": "drizzle-kit generate:mysql",
    "tb:pull": "tb pull --auto --force"
  },
  "dependencies": {
    "@hazel/integrations": "workspace:*",
    "@chronark/zod-bird": "^0.3.1",
    "@planetscale/database": "^1.11.0",
    "drizzle-orm": "^0.29.1",
    "drizzle-zod": "^0.5.1"
  },
  "peerDependencies": {
    "@hazel/utils": "workspace:*",
    "zod": "^3.x"
  },
  "devDependencies": {
    "@hazel/tsconfig": "workspace:*",
    "dotenv": "^16.3.1",
    "drizzle-kit": "^0.20.4",
    "typescript": "^5.3.2"
  },
  "exports": {
    "./*": "./src/*.ts",
    ".": "./src/index.ts",
    "./schema": "./src/schema.ts",
    "./utils": "./src/utils.ts",
    "./integrations": "./src/integrations.ts"
  },
  "typesVersions": {
    "*": {
      "*": [
        "src/*"
      ]
    }
  }
}