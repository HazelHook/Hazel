		"@types/react": "^18.2.31",
		"@types/react-dom": "^18.2.14",
		"eslint": "^8.51.0",
		"prettier": "^3.0.3",
		"@supabase/supabase-js": "^2.38.1",
		"typescript": "^5.2.2"
	},
	"exports": {
