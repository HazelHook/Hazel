{
  "name": "@hazelapp/sdk",
  "private": false,
  "version": "0.0.1",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "git clean -xdf .turbo node_modules",
    "format": "prettier --check \"**/*.{ts,tsx}\"",
    "build": "tsup",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@edge-runtime/ponyfill": "^2.4.1",
    "@edge-runtime/primitives": "^4.0.5",
    "@hazel/utils": "workspace:*",
    "cross-fetch": "^4.0.0",
    "next": "14.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "std-env": "^3.5.0",
    "tailwindcss": "3.3.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@hazel/tsconfig": "workspace:*",
    "@types/react": "18.2.31",
    "@types/react-dom": "^18.2.16",
    "eslint": "^8.54.0",
    "next": "14.0.4",
    "prettier": "^3.1.0",
    "react": "^18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.3.5",
    "tailwindcss-animate": "^1.0.7",
    "tsup": "8.0.1",
    "type-fest": "^4.8.2",
    "typescript": "^5.3.3"
  },
  "files": [
    "dist/**/*"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./next": {
      "import": "./dist/next.mjs",
      "require": "./dist/next.js",
      "types": "./dist/next.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/index.d.ts"
      ],
      "next": [
        "dist/next.d.ts"
      ]
    }
  }
}