{
	"doppler.autocomplete.enable": true,
	"doppler.hover.enable": true,
	"files.exclude": {
		"**/.git": false,
		"**/.svn": true,
		"**/.hg": true,
		"**/CVS": true,
		"**/.DS_Store": true,
		"**/Thumbs.db": true
	},
	"typescript.preferences.importModuleSpecifier": "shortest",
	"i18n-ally.localesPaths": ["apps/web/src/i18n/locales"],
	"i18n-ally.keystyle": "nested",
	"hazel.pagesDir": "apps/web/app/[locale]",
	"hazel.pageTypes": [
		{
			"name": "New",
			"icon": "new-file",
			"slug": "new"
		},
		{
			"name": "Details",
			"icon": "browser",
			"slug": "[id]"
		},
		{
			"name": "Events",
			"icon": "zap",
			"slug": "events"
		}
	],
	"hazel.indentation": "    ",
	"hazel.defaultIcon": ""
}
