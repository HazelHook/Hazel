{
	"extends": "@hazel/tsconfig/base.json",
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"@/*": ["./src/*"]
		},
		"plugins": [
			{
				"name": "next"
			}
		],
		"tsBuildInfoFile": "node_modules/.cache/tsbuildinfo.json"
	},
	"include": ["next-env.d.ts", ".next/types/**/*.ts", "*.ts", "*.mjs", "src"],
	"exclude": ["node_modules"]
}
