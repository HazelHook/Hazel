{
	"extends": "@hazel/tsconfig/base.json",
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"@/*": ["./src/*"]
		},
		"plugins": [
			{
				"name": "next"
			}
		],
		"tsBuildInfoFile": "node_modules/.cache/tsbuildinfo.json"
	},
	"include": [
		"next-env.d.ts",
		".next/types/**/*.ts",
		"*.ts",
		"*.mjs",
		"src",
		"../../packages/auth/src/utils/require-session.ts"
	],
	"exclude": ["node_modules"]
}
