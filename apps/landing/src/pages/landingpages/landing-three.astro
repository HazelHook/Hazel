---
import BaseLayout from "@/layouts/BaseLayout.astro";

import HeroThree from "@/components/headers/HeroThree.astro";
import FeatureOne from "@/components/features/FeatureOne.astro";
import LogoCloudThree from "@/components/testimonials/LogoCloudThree.astro";
import FeatureFive from "@/components/features/FeatureFive.astro";
import FeatureEight from "@/components/features/FeatureEight.astro";
import FeatureTwelve from "@/components/features/FeatureTwelve.astro";
import CtaOne from "@/components/ctas/CtaOne.astro";
import PricingFour from "@/components/pricing/PricingFour.astro";
import TestimonialOne from "@/components/testimonials/TestimonialOne.astro";
import { AstroSeo } from "@astrolib/seo";
import FeatureEleven from "@/components/features/FeatureEleven.astro";
---

<!---
Use AstroSeo in all the pages you want different Seo than the index page
-->
<AstroSeo
  title="Landing page 03"
  description="Your site description goes here"
  canonical="https://www.hazelhook.dev"
  openGraph={{
    url: "https://www.hazelhook.dev",
    title: "Open Graph Title",
    description: "Open Graph Description",
    images: [
      {
        url: "https://www.hazelhook.dev/opengraph/social-og.jpg",
        width: 1200,
        height: 630,
        alt: "Social open graph",
        type: "image/jpeg",
      },
      {
        url: "https://www.hazelhook.dev/opengraph/twitter-og.jpg",
        width: 4096,
        height: 4096,
        alt: "Twitter open graph",
        type: "image/jpeg",
      },
      { url: "https://www.hazelhook.dev/opengraph/social-og.jpg" },
      { url: "https://www.hazelhook.dev/opengraph/twitter-og.jpg" },
    ],
    site_name: "YourSitesName",
  }}
  twitter={{
    handle: "@makisuo__",
    site: "makisuo__",
    cardType: "summary_large_image",
  }}
/>
<BaseLayout>
  <HeroThree />
  <FeatureOne />
  <LogoCloudThree />
  <FeatureFive />
  <FeatureEight />
  <FeatureTwelve />
  <TestimonialOne />
  <PricingFour />
  <CtaOne />
</BaseLayout>
